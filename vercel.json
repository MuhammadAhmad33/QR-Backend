{
    "version": 2,
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js"
      }
    ],
    "env": {
      "DB_URI": "@db_uri",
      "JWT_SECRET": "@jwt_secret"
    }
  }
  